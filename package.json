{
    "name": "prosperity",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build:ci": "npm run check-lint && npm run check-ts && npm run build-only",
        "build-storybook:ci": "storybook build --quiet",
        "test-runner:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:127.0.0.1:6006 && npm run test-storybook:ci && npm run test:ci\"",
        "test-storybook:ci": "test-storybook --excludeTags='no-tests' --no-cache -- --coverage",
        "test:ci": "vitest --run",
        "check-lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
        "check-ts": "vue-tsc --build --force",
        "build-only": "vite build",
        "dev": "vite",
        "dev-test": "vitest",
        "fix-errors": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "fix-format": "prettier --write ./",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@storybook/test-runner": "^0.19.0",
        "@testing-library/vue": "^8.1.0",
        "axe-playwright": "^2.0.1",
        "axios": "^1.5.1",
        "concurrently": "^8.2.2",
        "pinia": "^2.1.7",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0",
        "vuetify": "^3.6.9"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.6.0",
        "@rushstack/eslint-patch": "^1.8.0",
        "@storybook/addon-essentials": "^8.1.11",
        "@storybook/addon-interactions": "^8.1.11",
        "@storybook/addon-links": "^8.1.11",
        "@storybook/blocks": "^8.1.11",
        "@storybook/test": "^8.1.11",
        "@storybook/vue3": "^8.1.11",
        "@storybook/vue3-vite": "^8.1.11",
        "@tsconfig/node20": "^20.1.4",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.12.5",
        "@vitejs/plugin-basic-ssl": "^1.0.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.57.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-vue": "^9.23.0",
        "jsdom": "^24.0.0",
        "npm-run-all2": "^6.1.2",
        "prettier": "^3.2.5",
        "sass": "^1.69.0",
        "storybook": "^8.1.11",
        "typescript": "~5.4.0",
        "vite": "^5.2.8",
        "vitest": "^1.6.0",
        "vue-tsc": "^2.0.11"
    }
}
